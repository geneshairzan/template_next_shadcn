{
  "type": "object",
  "description": "Extraction of 20 most recent articles from finance.detik.com and its subpages.",
  "properties": {
    "articles": {
      "type": "array",
      "description": "List of articles from finance.detik.com",
      "items": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "description": "Title of the article"
          },
          "author": {
            "type": "string",
            "description": "Author of the article (if available)"
          },
          "summary": {
            "type": "string",
            "description": "Summary of the article (max 100 words)"
          },
          "publication_date": {
            "type": "string",
            "description": "Publication date of the article"
          }
        },
        "required": ["title", "summary", "publication_date"]
      }
    }
  },
  "required": ["articles"]
}
